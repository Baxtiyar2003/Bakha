import java.util.*;
public class Main{
    public static void main(String[] args) {

        Scanner con = new Scanner(System.in);
        double m1 = con.nextDouble();
        double m2 = con.nextDouble();
        double F = con.nextDouble();
        double r = Math.sqrt(6.674e-11 * m1 * m2 / F);
        if (m1 > 0.0 && m2 > 0.0 && F > 0.0)
            System.out.printf("%3.e\n", r);
        else
            System.out.println("Error");
            con.close();
        
    }
}
